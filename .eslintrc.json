{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", {
      "trailingComma": "es5",
      "singleQuote": true,
      "bracketSpacing": true,
      "jsxBracketSameLine": true
    }]
  }
}